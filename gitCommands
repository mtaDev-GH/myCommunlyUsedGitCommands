git init

git status

git add .

git commit -m "<message>"

git log

git branch

git checkout <branchName>

git branch <newBranchName>

git checkout -b <newBranchName>

git remote

git remote add <localLabelForRemoteRepo> <url>

git push

git push -u <localLabelForRemoteRepo> <remoteBranchName>

git log --all --oneline --graph
